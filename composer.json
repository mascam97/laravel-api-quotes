{
    "name": "mascam97/laravel-api-quotes",
    "type": "project",
    "description": "Community to share and rate quotes.",
    "keywords": [
        "laravel8",
        "php8",
        "api-rest",
        "ratings",
        "test-driven-development",
        "laravel-sail",
        "laravel-sanctum",
        "phpstan",
        "query-builder"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mart√≠n S. Campos",
            "homepage": "https://github.com/mascam97"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel-lang/lang": "~8.0",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.9",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "3.0",
        "spatie/laravel-enum": "^3.0",
        "spatie/laravel-model-states": "^2.3",
        "spatie/laravel-query-builder": "5.0",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0"
    },
    "require-dev": {
        "eduarguz/shift-php-cs": "^3.0",
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.3",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "1.0",
        "phpunit/phpunit": "^9.3.3",
        "rector/rector": "^0.12.23",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Domain\\": "src/Domain/",
            "Support\\": "src/Support/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpstan": "vendor/bin/phpstan analyse",
        "rector": "vendor/bin/rector process src"
    }
}
